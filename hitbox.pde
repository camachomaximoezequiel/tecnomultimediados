void dibujarHitbox1(){
  
    FLine lineacorazon = new FLine(408, 170, 428, 170);
    
    lineacorazon.setGrabbable(false);
    lineacorazon.setName("boxcorazon");
    lineacorazon.setStroke(0, 255, 0);
    lineacorazon.setStrokeWeight(10);
    mundo.add(lineacorazon);
}

